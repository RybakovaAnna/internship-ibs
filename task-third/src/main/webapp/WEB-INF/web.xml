<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">

    <!--Greetings servlet -->
    <servlet>
        <servlet-name>GetJsonServlet</servlet-name>
        <servlet-class>com.gmail.rybachko.anna.taskthird.servlets.GetJsonServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>GetJsonServlet</servlet-name>
        <url-pattern>/get</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>JsonServiceServlet</servlet-name>
        <servlet-class>com.gmail.rybachko.anna.taskthird.servlets.JsonServiceServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>JsonServiceServlet</servlet-name>
        <url-pattern>/jsonService</url-pattern>
    </servlet-mapping>
</web-app>